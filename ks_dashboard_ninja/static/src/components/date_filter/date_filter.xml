    <t t-name="ks_dashboard_ninja.ks_date_filter_dropdown">
        <Dropdown menuClass="'ks-dropdown-menu'">
            <t t-set-slot="content">
                <DropdownItem t-foreach="dateFilters" t-as="dateFilter" t-key="dateFilter_index"
                                class="{ 'global-active': state.config.dateSelection === dateFilter , 'd-none': dateFilter === 'l_custom' &amp;&amp; isMobile}"
                               onSelected="() => this.dateFilterSelect(dateFilter)">
                    <t t-esc="dateFilters[dateFilter]"/>
                </DropdownItem>
            </t>
            <span class="ellipsis-content d-block max-width-medium-6vw" t-esc="dateFilters[state.config.dateSelection]"/>
        </Dropdown>
    </t>

</templates>