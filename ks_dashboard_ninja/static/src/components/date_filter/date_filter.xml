            <t t-if="!state.showCustomDatesSymbol and state.showCustomDates">
                <div class="ks_date_input_fields d-flex gap-2">
                    <div class="form-input-box form-control start-date">
                         <DateTimeInput value="startDate" type="'datetime'" rounding="1"
                                   id="'ks_btn_middle_child'" placeholder="'Start Date...'" onChange.bind="changeStartDate"/>
                    </div>
                    <div class="form-input-box form-control start-date">
                        <DateTimeInput value="endDate" type="'datetime'" rounding="1"
