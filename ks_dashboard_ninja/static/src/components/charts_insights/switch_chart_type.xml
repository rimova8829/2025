<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="ks_dashboard_ninja.switch_chart_type">
        <div class="ks-common-tab-pills">
            <ul class="nav nav-pills mb-3 border-bottom border-2" id="pills-tab" role="tablist">
                <t t-set="t_getItemTypes" t-value="this.getItemTypes()"/>
                <li t-foreach="t_getItemTypes" t-as="t_getItemType" t-key="t_getItemType_index">
                    <button class="nav-link text-primary fw-semibold position-relative" t-att-class="{ 'active': this.state.itemType === t_getItemType }"
                        id="pills-home-tab" data-bs-toggle="pill" data-bs-target="#pills-one" type="button"
                        role="tab" aria-controls="pills-home" aria-selected="true"
                        t-on-click="() => this.switchItem(t_getItemType)">
                        <t t-call="{{t_getItemType}}"/>
                    </button>
                </li>
            </ul>
        </div>
    </t>

